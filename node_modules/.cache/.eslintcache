ame=\"col me-4 ms-4\">\n      <br></br>\n      <div class=\"form-check\">\n        <input\n          class=\"form-check-input me-3\"\n          type=\"checkbox\"\n          value=\"\"\n          id=\"flexCheckChecked\"\n          checked\n        />\n        <label class=\"form-check-label\" for=\"flexCheckChecked\">\n          <h1>{title}</h1>\n        </label>\n      </div>\n\n      <div className=\"pb-3\">\n        <input\n          id=\"search\"\n          type=\"text\"\n          placeholder=\"SEARCH\"\n          value={filter}\n          onChange={(e) => setFilter(e.target.value)}\n        />\n      </div>\n\n      {tasks\n        .filter(\n          (item) =>\n            item.status === status &&\n            item.isCompleted === false &&\n            item.title.toLowerCase().includes(filter.toLowerCase())\n        )\n        .map((item) => (\n          <div\n            draggable\n            droppable\n            onDragStart={(e) => onDragStart(e, item)}\n            onDragOver={(e) => draggingOver(e)}\n            onDrop={(e) => onDrop(e)}\n            className=\"list-item container pt-2\"\n          >\n            <ul className=\"list-unstyled\">\n              <li className=\"list-container d-flex mb-3\">\n                <div className=\"align-self-center form-check form-check-inline\">\n                  <input\n                    className=\"form-check-input\"\n                    id=\"btn-radio\"\n                    type=\"radio\"\n                    onClick={() => handleStatus(item.id)}\n                  />\n\n                  <label class=\"form-check-label\" for=\"inlineRadio1\"></label>\n                </div>\n\n                <span className=\"align-self-center text-start\">\n                  {item.title}\n                </span>\n\n                <span className=\"ms-auto p-1 btn btn-primary\" id=\"btn-day\">\n                  {item.day}\n                </span>\n\n                <button\n                  type=\"button\"\n                  onClick={() => deleteTask(item.id)}\n                  class=\"align-self-center btn btn-close btn-close-white\"\n                  aria-label=\"Close\"\n                  id=\"btn-delete\"\n                ></button>\n              </li>\n            </ul>\n          </div>\n        ))}\n\n      <div className=\"pt-2\">\n        <button\n          type=\"button\"\n          class=\"btn btn-blue btn btn-outline-primary bi bi-plus blue-color\"\n          id=\"newTask\"\n          onClick={createTask}\n          disabled={!newTask}\n        ></button>\n\n        <input\n          id=\"new\"\n          type=\"text\"\n          placeholder=\"Create new task\"\n          value={newTask}\n          onChange={(e) => setNewTask(e.target.value)}\n        />\n\n        <select\n          className=\"btn btn-primary mb-1\"\n          id=\"select\"\n          value={day}\n          onChange={(e) => setDay(e.target.value)}\n        >\n          {weekDays.map((item) => (\n            <option value={item}>{item}</option>\n          ))}\n        </select>\n      </div>\n    </div>\n  );\n};\n\nexport default NotCompleted;\n","/Users/andreeacsecs/Desktop/to-do-app-push/to-do-app/to-do-app/src/components/Footer.jsx",[],[],{"ruleId":"34","severity":1,"message":"35","line":54,"column":26,"nodeType":"36","messageId":"37","endLine":54,"endColumn":28},"eqeqeq","Expected '===' and instead saw '=='.","BinaryExpression","unexpected"]